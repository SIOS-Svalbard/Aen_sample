<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Samples from station</title>
<frx:category>Aen</frx:category>
<frx:options/>
<frx:parameters>
  <frx:parm id="stationname" label="Station Name" require="1" desc="The name of the station" data_source="AeN/unique_stations" data_field="stationname" type="select" />
</frx:parameters>
<frx:docgen>
  <frx:doc type="csv"/>
</frx:docgen>
<frx:fields>

  <frx:field id="eventid" link="reports/ind_sample/?eventid={eventid}"/>

  <frx:field id="parenteventid" link="reports/ind_sample/?eventid={pareteventid}"/>

  <frx:field id="sampletype" link="reports/aen_sampletype/?sampletype={sampletype}"/>

  <frx:field id="geartype" link="reports/aen_gear/?geartype={geartype}"/>

  <frx:field id="stationname" link="reports/aen_station/?stationname={stationname}"/>

  <frx:field id="eventdate" link="reports/aen_date/?eventdate=&quot;{eventdate}&quot;"/>
</frx:fields>
<frx:menu/>
<frx:cache/>
<style/>
</head>
<body>
<div id="search_station_block" class="FrxTable" frx:block="AeN/search_station">
    <table>
      <thead>
        <tr>
          <th>Event Date</th>
          <th>Station Name</th>
          <th>Gear Type</th>
          <th>Sample Type</th>
          <th>Event ID</th>
          <th>Parent Event ID</th></tr></thead>
      <tbody>
        <tr id="search_station" frx:foreach="*">
          <td>{eventdate}</td>
          <td>{stationname}</td>
          <td>{geartype}</td>
          <td>{sampletype}</td>
          <td>{eventid}</td>
          <td>{parenteventid}</td></tr></tbody></table></div></body>
</html>
